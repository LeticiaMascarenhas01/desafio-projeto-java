programa {
  funcao inicio() {
    real num1, num2, soma, sub, div, multi

    escreva("Primeiro numero: ", "\n", num1)
    leia (num1)

    escreva("Segundo numero: ", "\n", num2)
    leia (num2)

    soma = num1 + num2
    sub = num1 - num2
    div = num1 / num2
    multi = num1 * num2

    escreva("O resultado da soma e: ", soma,"\n")
    escreva("O resultado da sub e: ", sub, "\n")
    escreva("O resultado da div e: ", div, "\n")
    escreva("O resultado da multi e: ", multi, "\n")
  }
}
